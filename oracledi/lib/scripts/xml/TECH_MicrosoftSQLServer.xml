	<Field name="Txt" type="java.lang.String"><![CDATA[select
	T.constraint_name CHECK_NAME,
	check_clause CHECK_TXT,
	1 CHECK_STATUS,
	'' CHECK_DESC
from Information_Schema.table_Constraints T, Information_Schema.check_Constraints C
where T.constraint_TYPE='CHECK'
and C.constraint_name=T.constraint_name
and T.table_schema = :SCHEMA
and T.table_name = :TABLE]]></Field>
