	<Field name="Txt" type="java.lang.String"><![CDATA[select I.name index_name, index_col(:TABLE, K.indid, K.keyno) column_name
from sysindexes I , sysindexkeys K
where I.id = object_id (:TABLE) 
and I.indid > 0 
and I.indid < 255 
and (I.status & 64)=0 
and I.id=K.id
and I.indid=K.indid
order by I.indid
]]></Field>
