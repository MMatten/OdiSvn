		AND Myindex.indisprimary != 't'
		AND MyindexClass.relkind = 'i' 
		AND MyindexClass.oid = Myindex.indexrelid
		AND Mytable.oid = Mycolumn.attrelid
		AND Mycolumn.attnum>0
		AND Mycolumn.attnum = Myindex.indKey[1]
union
