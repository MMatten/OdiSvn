	<Field name="ITxt" type="com.sunopsis.sql.DbInt"><![CDATA[2413999]]></Field>
	<Field name="ITxtOrig" type="com.sunopsis.sql.DbInt"><![CDATA[5]]></Field>
	<Field name="Txt" type="java.lang.String"><![CDATA[
SELECT 
i.INDEX_NAME  INDEX_NAME,
k.COLUMN_NAME COLUMN_NAME
FROM QSYS2.SYSKEYS k,  
QSYS2.SYSINDEXES i,  
QSYS2.SYSCOLUMNS c 
WHERE k.INDEX_SCHEMA=i.INDEX_SCHEMA 
AND k.INDEX_NAME=i.INDEX_NAME 
AND i.TABLE_SCHEMA=c.TABLE_SCHEMA 
AND i.TABLE_NAME=c.TABLE_NAME 
AND k.COLUMN_NAME=c.COLUMN_NAME 
AND i.IS_UNIQUE = 'D'
AND i.TABLE_SCHEMA = :SCHEMA
AND i.TABLE_NAME = :TABLE
ORDER BY  k.ORDINAL_POSITION 
]]></Field>
</Object>
