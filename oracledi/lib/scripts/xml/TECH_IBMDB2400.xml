	<Field name="ITxt" type="com.sunopsis.sql.DbInt"><![CDATA[2524999]]></Field>
	<Field name="ITxtOrig" type="com.sunopsis.sql.DbInt"><![CDATA[16]]></Field>
	<Field name="Txt" type="java.lang.String"><![CDATA[select	PK_TABLE.TABLE_NAME 	PK_TABLE_NAME,
	REF.CONSTRAINT_NAME FK_NAME, 
	FK_TABLE.COLUMN_NAME FK_COLUMN_NAME,
	PK_TABLE.COLUMN_NAME PK_COLUMN_NAME
from	QSYS2.SYSREFCST  as REF,
	QSYS2.SYSKEYCST as FK_TABLE,
	QSYS2.SYSKEYCST as PK_TABLE
where	REF.CONSTRAINT_SCHEMA = FK_TABLE.CONSTRAINT_SCHEMA
and	REF.CONSTRAINT_NAME = FK_TABLE.CONSTRAINT_NAME
and	REF.UNIQUE_CONSTRAINT_SCHEMA = PK_TABLE.CONSTRAINT_SCHEMA
and	REF.UNIQUE_CONSTRAINT_NAME = PK_TABLE.CONSTRAINT_NAME
and	PK_TABLE.ORDINAL_POSITION = FK_TABLE.ORDINAL_POSITION
and	FK_TABLE.TABLE_NAME =:TABLE
and	FK_TABLE.TABLE_SCHEMA = :SCHEMA]]></Field>
</Object>
