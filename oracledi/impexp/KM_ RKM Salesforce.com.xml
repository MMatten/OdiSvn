class Log:
    """Creates a log file and traces information
    """
    def __init__(self, logfilename, uselog=1, append=0, printtoscreen=0):
        self.logfilename = logfilename
        self.printtoscreen = printtoscreen
        self.uselog=uselog
        if self.uselog:
            if not append: # Reset the file
                f = open(logfilename, "w")
                f.close()
    
    def record(self, rec):
        if self.uselog:
            t = time.localtime()
            l = '%d-%02d-%02d %02d:%02d:%02d - %s' % (t[0], t[1], t[2], t[3], t[4], t[5], rec)
            f = open(self.logfilename, "a+")
            print >> f, l
            f.close()
            if self.printtoscreen:
                print l


class sforceTables:
    def __init__(self,con,logger):
        self.queryInsTable = ''' INSERT INTO SNP_REV_TABLE
                    (I_MOD,TABLE_NAME,RES_NAME,TABLE_ALIAS,TABLE_TYPE,TABLE_DESC,IND_SHOW,R_COUNT,FILE_FORMAT,FILE_SEP_FIELD,FILE_ENC_FIELD,FILE_SEP_ROW, FILE_FIRST_ROW, FILE_DEC_SEP, SMOD_CODE) 
                    VALUES(?, ?, ?, ?, 'T', '', '1', NULL, '', '', '', '', NULL, '', NULL) '''    
                    
        self.queryInsColumn = ''' INSERT INTO SNP_REV_COL
                    (I_MOD, TABLE_NAME, COL_NAME, COL_HEADING, COL_DESC, DT_DRIVER, 
                    POS, LONGC, SCALEC, FILE_POS, BYTES, IND_WRITE, COL_MANDATORY, 
                    CHECK_FLOW, CHECK_STAT, COL_FORMAT, COL_DEC_SEP, REC_CODE_LIST, COL_NULL_IF_ERR, DEF_VALUE) 
                    VALUES(?, ?, ?, ?, ?, ?, ?, ?, ?, NULL, ?, '1', ?, '1', '1', NULL, NULL, NULL, NULL, NULL) '''
        self.queryInsPK = ''' INSERT INTO SNP_REV_KEY
                (I_MOD, TABLE_NAME, KEY_NAME, CONS_TYPE, IND_ACTIVE, CHECK_FLOW, CHECK_STAT)
                VALUES(?, ?, ? , 'PK', '1', '1', '1') '''
                
        self.queryInsPKCol = ''' INSERT INTO SNP_REV_KEY_COL
                    (I_MOD, TABLE_NAME, KEY_NAME, COL_NAME, POS) 
                    VALUES (?, ?, ?, ?, ?) '''

        self.queryInsRef = ''' INSERT INTO SNP_REV_JOIN
                (I_MOD,FK_NAME,TABLE_NAME,FK_TYPE,PK_CATALOG,PK_SCHEMA,PK_TABLE_NAME,CHECK_STAT,CHECK_FLOW,IND_ACTIVE, DEFER,UPD_RULE, DEL_RULE)
                VALUES
                (?, ?, ?, 'D', NULL, NULL, ?, '1', '1','1', NULL, 'R', 'R') '''

        self.queryInsRefCol = ''' INSERT INTO SNP_REV_JOIN_COL
                (I_MOD, FK_NAME, FK_COL_NAME, FK_TABLE_NAME, PK_COL_NAME, PK_TABLE_NAME, POS)
                VALUES
                (?, ?, ?, ?, 'Id', ?, ?) '''   
        self.con=con
        self.tables = []
        self.logger=logger
        
    def toString(self):
        for t in self.tables:
            t.asString()
    def toSQL(self,i_mod):        
        psmt =  self.con.prepareStatement(self.queryInsTable)
        psmc =  self.con.prepareStatement(self.queryInsColumn)
        psmr =  self.con.prepareStatement(self.queryInsRef)
        psmrc =  self.con.prepareStatement(self.queryInsRefCol)
        psmpk = self.con.prepareStatement(self.queryInsPK)
        psmpkc = self.con.prepareStatement(self.queryInsPKCol)        
        for t in self.tables:
            sf_label=""
            try:
                psmt.clearParameters()   
                psmt.setInt(1,i_mod)   
                psmt.setString(2,"%s" % t.getLabel()) 
                psmt.setString(3,"%s" % t.getName()) 
                psmt.setString(4,"%s" % t.getAlias())    
                psmt.executeUpdate()
                sf_label = t.getLabel()

            except:
                psmt.clearParameters()   
                psmt.setInt(1,i_mod)   
                psmt.setString(2,"%s" % t.getLabel()) 
                psmt.setString(3,"%s" % t.getLabel()) 
                psmt.setString(4,"%s" % t.getAlias())    
                psmt.executeUpdate() 
                sf_label = t.getLabel()      
            self.logger.record( 'Inserted table %s ' % (sf_label))
               
            
            col_counter=0    
            counter=0
            for c in t.getColumns():
                col_counter+=1
                ref_counter=counter
                counter=c.toSQL(psmc,psmr,psmrc,psmpk,psmpkc,i_mod,sf_label,col_counter,ref_counter,self.logger)
    def addTable(self,table):
        self.tables.append(table)
        

class sforceTable:
    """
    Sales force container Class for tables
    """
    def __init__(self, name,label):
        self.name = name
        self.label = label
        self.alias = label
        self.columns = []
    def getName(self):
        return (self.name)
    def getLabel(self):
        return(self.label)
    def getAlias(self):
        return(self.alias)
    def getColumns(self):
        return (self.columns)
    def asString(self):
        """
        helper method that displays a record as a string
        """
        print "Sforce table: [name=%s, label=%s, alias=%s]" %(self.name, self.label, self.alias)
        for c in self.columns:
            print c.asString()
        
    def addColumns(self,col):
        """
        list of columns
        """
        self.columns.append(col)
                    
        
        
class sforceColumn:
    """
    Sales force container Class for tables
    """
    def __init__(self, colname,coltype,collength,colprecision,colbyte,colnull,flabel,reference):
        self.colname = colname
        self.coltype = coltype
        self.collength = collength
        self.colprecision = colprecision
        self.colbyte = colbyte
        self.colnull = colnull                        
        self.reference = reference
        self.flabel=flabel.replace("'","''")
    
    def getColName(self):
        return self.colname
    def getType(self):
        return self.coltype
                   
    def asString(self):
        """
        helper method that displays a record as a string
        """
        return "Sforce columns: [colname=%s, coltype=%s, collength=%s, reference=%s]" %(self.colname, self.coltype, self.collength, self.reference)
    def toSQL(self,psmc,psmr,psmrf,psmpk,psmpkc,i_mod,sf_label,sf_pos,ref_cnt,logger):
        logger.record("Sforce columns: [colname=%s, coltype=%s, collength=%s, reference=%s, label %s]" %(self.colname, self.coltype, self.collength, self.reference,sf_label))
        psmc.clearParameters()  
        psmc.setInt(1,i_mod)   
        psmc.setString(2,"%s" %sf_label)  
        psmc.setString(3,"%s" %self.colname)  
        psmc.setString(4,"%s" %self.colname[:35])  
        psmc.setString(5,"%s" %self.flabel)  
        psmc.setString(6,"%s" %self.coltype)  
        psmc.setInt(7,sf_pos)  
        psmc.setInt(8,int("%s" %self.collength))  
        psmc.setInt(9,int("%s" %self.colprecision ))  
        psmc.setInt(10,int("%s" %self.colbyte))  
        psmc.setString(11,"%s" %self.colnull)
        psmc.executeUpdate()
        
        if '%s' % (self.coltype)=='id':
            logger.record ("pk insertion %s %s %s" %(self.colname,self.coltype,sf_label))
            psmpk.clearParameters()  
            psmpk.setInt(1,i_mod)   
            psmpk.setString(2,"%s" % sf_label)  
            psmpk.setString(3,"PK_%s_ID" %sf_label.replace(' ','').upper())  
            psmpk.executeUpdate()
            
            psmpkc.clearParameters()    
            psmpkc.setInt(1,i_mod)   
            psmpkc.setString(2,"%s" % sf_label)  
            psmpkc.setString(3,"PK_%s_ID" %sf_label.replace(' ','').upper())  
            psmpkc.setString(4,"%s" %self.colname)  
            psmpkc.setInt(5,1)  
            psmpkc.executeUpdate()

        ri=0
        ref_counter=ref_cnt
        for r in self.reference:
            ref_counter+=1
            ri+=1
            psmr.clearParameters()
            psmr.setInt(1,i_mod)   
            psmr.setString(2,"FK_%s%s%s" %(sf_label.upper(),r.upper(),ref_counter))  
            psmr.setString(3,"%s" %sf_label)  
            psmr.setString(4,r)   
            psmr.executeUpdate()
            
            psmrf.clearParameters()
            psmrf.setInt(1,i_mod)   
            psmrf.setString(2,"FK_%s%s%s" %(sf_label.upper(),r.upper(),ref_counter))  
            psmrf.setString(3,"%s" %self.colname)  
            psmrf.setString(4,"%s" %sf_label)  
            psmrf.setString(5,r)  
            psmrf.setInt(6,ri) 
            psmrf.executeUpdate()
        return ref_counter
            

    

def login(user, pwd,logger):
    #Create binding object for sforce
    try:
        sfdc = SforceServiceLocator().getSoap()
        sfdc.setTimeout(60000);
    except Exception, e:
         logger.record( "Error creating binding object : %s" % str(e))
       #login
    try:
        loginResult = sfdc.login(user, pwd)
    except  Exception, e:
         logger.record( "Error loging in :%s" % str(e.getExceptionCode()))
    else:
         logger.record( "Logged in successfully using UserID: %s" % (loginResult.getUserId()))
         logger.record( "                      and SessionID: %s" % (loginResult.getSessionId()))
    #Reset the SOAP endpoint to the returned server URL
    try:
        sfdc = SforceServiceLocator().getSoap(java.net.URL(loginResult.getServerUrl()))
        logger.record( "Reset SOAP endpoint")
    except:
         logger.record( "Error reseting the SOAP endpoint")

    #Create a new session header object 
    #add the session ID returned from the login
    try:  
        sh = enterprise.SessionHeader()
    except:
        sh = enterprise._SessionHeader()
    sh.setSessionId(loginResult.getSessionId())
    logger.record( "Created session header")
    logger.record( "Set the session header for subsequent call authentication")
    #Set the session header for subsequent call authentication  
    sfdc.setHeader(SforceServiceLocator().getServiceName().getNamespaceURI(),"SessionHeader", sh)
    logger.record( "Set header")
 
        
    
    return sfdc

def getTypes(session):
    describeGlobalResult = session.describeGlobal()
    types=[]
    if describeGlobalResult<>'':
        #Get the array of object names from the result 
        _types=describeGlobalResult.getTypes()
        
        
        for type in _types:
            descSObjectRslt = session.describeSObject(type)
            Fields = []
            Fields = descSObjectRslt.getFields()
            Table = sforceTable(descSObjectRslt.getLabel(),descSObjectRslt.getName())
            reverse.addTable(Table)
            for field in Fields:
                ref=[]
                if '%s' % (field.getType())=='reference':
                    ref = field.getReferenceTo()
                Field = sforceColumn(field.getName(),field.getType(),field.getLength(),field.getPrecision(),field.getByteLength(),(field.isNillable()-1)*-1,field.getLabel(),ref)
                Table.addColumns(Field)
        return
    

