import oracle.odi.sap.km.test.JCoTest;

String log =JCoTest.testSAPConnection("<%=odiRef.getInfo("SRC_DSERV_NAME")%>", 
	"<?=odiRef.getFlexFieldValue( odiRef.getInfo("SRC_I_CONNECT"), "1400", "SAP_SYSTEM_NUMBER" )?>",
	"<?=odiRef.getFlexFieldValue( odiRef.getInfo("SRC_I_CONNECT"), "1400", "SAP_CLIENT" )?>", 
	"<?=odiRef.getFlexFieldValue( odiRef.getInfo("SRC_I_CONNECT"), "1400", "SAP_LANGUAGE" )?>",
	"<%=odiRef.getInfo("SRC_USER_NAME")%>", 
	"<%=odiRef.getInfo("SRC_PASS")%>"
);

throw new Exception( log );
]]></Field>
